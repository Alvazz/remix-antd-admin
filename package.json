{
  "name": "remix-antd-admin",
  "version": "0.0.5",
  "description": "A Remix Antd Admin",
  "type": "module",
  "keywords": [
    "remix",
    "admin",
    "echarts",
    "i18n",
    "css-in-js",
    "remix-utils",
    "react-mind",
    "react-mindmap",
    "react-wordcloud",
    "reactflow",
    "echarts-liquidfill-ssr",
    "ssr",
    "copper",
    "prettier",
    "antd",
    "pro-components",
    "antd pro"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yyong008/remix-antd-admin.git"
  },
  "author": "Magnesium",
  "scripts": {
    "build": "remix build",
    "coverage": "vitest run --coverage",
    "dev": "remix dev --manual",
    "lint": "eslint ./app",
    "format": "prettier --write app/ public/locales",
    "start": "remix-serve ./build/index.js",
    "test": "vitest",
    "typecheck": "tsc",
